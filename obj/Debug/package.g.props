<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <PackageJsonName Condition="$(PackageJsonName) == ''">tiny-https-server</PackageJsonName>
    <PackageJsonVersion Condition="$(PackageJsonVersion) == ''">2.0.0-beta1</PackageJsonVersion>
    <PackageJsonLicense Condition="$(PackageJsonLicense) == ''">MIT</PackageJsonLicense>
    <PackageJsonDescription Condition="$(PackageJsonDescription) == ''">Tiny web server with HTTPS support, static file serving, subdomain support, middleware support, and service worker support.</PackageJsonDescription>
    <PackageJsonKeywords Condition="$(PackageJsonKeywords) == ''">[
  "tiny",
  "web",
  "server",
  "webserver",
  "http",
  "https",
  "static",
  "subdomain",
  "middleware",
  "fast",
  "sw",
  "service-worker",
  "pwa",
  "progressive",
  "app",
  "manifest",
  "favicon",
  "robots",
  "sitemap",
  "seo"
]</PackageJsonKeywords>
    <PackageJsonBrowser Condition="$(PackageJsonBrowser) == ''">browser.js</PackageJsonBrowser>
    <PackageJsonMain Condition="$(PackageJsonMain) == ''">index.js</PackageJsonMain>
    <PackageJsonScriptsTest Condition="$(PackageJsonScriptsTest) == ''">node index.test</PackageJsonScriptsTest>
    <PackageJsonScriptsStart Condition="$(PackageJsonScriptsStart) == ''">node index.js</PackageJsonScriptsStart>
    <PackageJsonBinTinyHttpsServer Condition="$(PackageJsonBinTinyHttpsServer) == ''">bin/tiny-https-server.js</PackageJsonBinTinyHttpsServer>
    <PackageJsonAuthor Condition="$(PackageJsonAuthor) == ''">Manuel Lõhmus &lt;manuel@hauss.ee&gt;</PackageJsonAuthor>
    <PackageJsonFiles Condition="$(PackageJsonFiles) == ''">[
  "bin/tiny-https-server.js",
  "cert/localhost-cert.pem",
  "cert/localhost-key.pem",
  "public/www/favicon.ico",
  "public/www/favicon-192x192.png",
  "public/www/favicon-512x512.png",
  "public/www/index.html",
  "public/www/pwa.manifest.json",
  "public/www/robots.txt",
  "public/www/sitemap.xml",
  "browser.js",
  "index.js",
  "index.test.js",
  "LICENSE",
  "mimeTypes.js",
  "package.json",
  "README.md"
]</PackageJsonFiles>
    <PackageJsonHomepage Condition="$(PackageJsonHomepage) == ''">https://github.com/manuel-lohmus/tiny-https-server#readme</PackageJsonHomepage>
    <PackageJsonBugsUrl Condition="$(PackageJsonBugsUrl) == ''">https://github.com/manuel-lohmus/tiny-https-server/issues</PackageJsonBugsUrl>
    <PackageJsonRepositoryType Condition="$(PackageJsonRepositoryType) == ''">git</PackageJsonRepositoryType>
    <PackageJsonRepositoryUrl Condition="$(PackageJsonRepositoryUrl) == ''">git+https://github.com/manuel-lohmus/tiny-https-server.git</PackageJsonRepositoryUrl>
    <PackageJsonDependenciesConfigSets Condition="$(PackageJsonDependenciesConfigSets) == ''">^3.0.0-beta.3</PackageJsonDependenciesConfigSets>
    <PackageJsonDependenciesDataContext Condition="$(PackageJsonDependenciesDataContext) == ''">^2.0.0-beta.6</PackageJsonDependenciesDataContext>
    <PackageJsonFunding Condition="$(PackageJsonFunding) == ''">https://manuel-lohmus.github.io/tiny-https-server/README.html#license</PackageJsonFunding>
  </PropertyGroup>
</Project>